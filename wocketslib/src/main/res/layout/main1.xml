<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
	android:orientation="vertical" android:layout_width="fill_parent"
	android:layout_height="fill_parent" android:gravity="center_horizontal">
	<LinearLayout android:orientation="horizontal"
		android:layout_width="fill_parent" android:layout_height="wrap_content">
		<TextView android:layout_width="fill_parent"
			android:layout_height="wrap_content" android:text="phoneID:"
			android:layout_weight="2" />
		<EditText android:id="@+id/phoneid1" android:layout_width="fill_parent"
			android:layout_height="wrap_content" android:layout_weight="1" />
	</LinearLayout>
	<LinearLayout android:orientation="horizontal"
		android:layout_width="fill_parent" android:layout_height="wrap_content">
		<TextView android:layout_width="fill_parent"
			android:layout_height="wrap_content" android:text="Start Date:"
			android:layout_weight="2" />
		<EditText android:id="@+id/startdate" android:layout_width="fill_parent"
			android:layout_height="wrap_content" android:layout_weight="1" />
	</LinearLayout>
	<LinearLayout android:orientation="horizontal"
		android:layout_width="fill_parent" android:layout_height="wrap_content">
		<TextView android:layout_width="fill_parent"
			android:layout_height="wrap_content" android:text="End Date:"
			android:layout_weight="2" />
		<EditText android:id="@+id/enddate" android:layout_width="fill_parent"
			android:layout_height="wrap_content" android:layout_weight="1" />
	</LinearLayout>
	<LinearLayout android:orientation="horizontal"
		android:layout_width="fill_parent" android:layout_height="wrap_content">
		<Button android:id="@+id/wallpaperBtn" android:layout_width="fill_parent"
			android:layout_height="wrap_content" android:text="Wallpaper"
			android:layout_weight="1" />
		<Button android:id="@+id/foodtrackBtn" android:layout_width="fill_parent"
			android:layout_height="wrap_content" android:text="Food Tracker"
			android:layout_weight="1" />
		<Button android:id="@+id/paBtn" android:layout_width="fill_parent"
			android:layout_height="wrap_content" android:text="PA Trackers"
			android:layout_weight="1" />
	</LinearLayout>
	<LinearLayout android:orientation="horizontal"
		android:layout_width="fill_parent" android:layout_height="wrap_content">
		<Button android:id="@+id/goalBtn" android:layout_width="fill_parent"
			android:layout_height="wrap_content" android:text="Goal Setting"
			android:layout_weight="1" />
		<Button android:id="@+id/weightingBtn" android:layout_width="fill_parent"
			android:layout_height="wrap_content" android:text="Weighting"
			android:layout_weight="1" />
		<Button android:id="@+id/copyBtn" android:layout_width="fill_parent"
			android:layout_height="wrap_content" android:text="Copy DB to Sdcard"
			android:layout_weight="1" />
	</LinearLayout>
	<LinearLayout android:orientation="horizontal"
		android:layout_width="fill_parent" android:layout_height="wrap_content">
		<Button android:id="@+id/testConnBtn" android:layout_width="fill_parent"
			android:layout_height="wrap_content" android:text="Conn Detector"
			android:layout_weight="1" />
		<Button android:id="@+id/testServerBtn" android:layout_width="fill_parent"
			android:layout_height="wrap_content" android:text="Server Detector"
			android:layout_weight="1" />
		<Button android:id="@+id/copyBtn1" android:layout_width="fill_parent"
			android:layout_height="wrap_content" android:text=""
			android:layout_weight="1" />
	</LinearLayout>
	<LinearLayout android:orientation="horizontal"
		android:layout_width="fill_parent" android:layout_height="wrap_content">
		<Button android:id="@+id/setDataBtn" android:layout_width="fill_parent"
			android:layout_height="wrap_content" android:text="Set Data"
			android:layout_weight="1" />
		<Button android:id="@+id/getDataBtn" android:layout_width="fill_parent"
			android:layout_height="wrap_content" android:text="Get Data"
			android:layout_weight="1" />
		<Button android:id="@+id/keepUpBtn" android:layout_width="fill_parent"
			android:layout_height="wrap_content" android:text="Keep Updating"
			android:layout_weight="1" />
	</LinearLayout>
	<TextView android:id="@+id/status1" android:layout_width="fill_parent"
		android:layout_height="wrap_content" />
</LinearLayout>
