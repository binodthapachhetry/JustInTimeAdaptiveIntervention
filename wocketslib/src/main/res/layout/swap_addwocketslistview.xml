<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
  xmlns:android="http://schemas.android.com/apk/res/android"
  android:orientation="horizontal" 
  android:layout_width="wrap_content"
  android:layout_height="60dp">
  	<ImageView android:layout_width="50dp" android:layout_gravity="center"
  	android:layout_height="50dp" android:id="@+id/addwockets_itemIcon" android:layout_marginLeft="10dp"
  	android:layout_marginTop="5dp"/>
  	
 	<TextView android:layout_width="200dp" android:layout_gravity="center" android:textSize="25dp"
  	android:layout_height="30dp" android:id="@+id/addwockets_itemTitle" android:layout_marginLeft="20dp"
  	android:layout_toRightOf="@+id/addwockets_itemIcon" android:layout_alignTop="@+id/addwockets_itemIcon"/>
 	
 	<TextView android:layout_width="180dp" android:layout_gravity="center" android:textSize="15dp"
  	android:layout_height="20dp" android:id="@+id/addwockets_itemID" android:layout_marginLeft="30dp" 
  	android:textColor="#D3D3D3" android:layout_below="@+id/addwockets_itemTitle" android:layout_toRightOf="@+id/addwockets_itemIcon"/>
 		
</RelativeLayout>
