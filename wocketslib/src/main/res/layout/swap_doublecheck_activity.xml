<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
	android:orientation="vertical" android:layout_width="fill_parent"
	android:layout_height="fill_parent" android:background="#FFF">
    <TextView android:textSize="30dp" android:layout_height="40dp"
		android:id="@+id/doubleCheck" android:text="@string/doubleCheck"
		android:layout_width="wrap_content" android:textStyle="bold" android:textColor="#365370"
		  android:layout_centerHorizontal="true" android:layout_marginTop="20dp" />
		  
	<TextView android:layout_height="wrap_content" android:layout_width="290dp"
		android:id="@+id/pageseventeen_text" android:textSize="25dp" android:layout_centerHorizontal="true"
		 android:layout_marginTop="100dp" android:textColor="#000"/>
		 
	<Button android:layout_height="60dp" android:textSize="25dp"  android:text="No. Fix it"
		android:id="@+id/page17no" android:layout_width="160dp" 
		android:layout_alignParentBottom="true" android:layout_alignParentLeft="true"/>
		
	<Button android:layout_height="60dp" android:text="Yes"
		android:textSize="25dp" android:id="@+id/page17yes" android:layout_width="160dp"
		android:layout_alignBaseline="@+id/page17no"
		android:layout_alignBottom="@+id/page17no" android:layout_alignParentRight="true" />
</RelativeLayout>
